{
  "name": "generic-lsp",
  "description": "Generic LSP Language Client",
  "author": "CapsAdmin",
  "license": "MIT",
  "version": "0.0.1",
  "publisher": "unpublished",
  "keywords": [
    "LSP",
    "Language Server Client"
  ],
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:lua",
    "workspaceContains:**/*.tl"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Generic LSP configuration",
      "properties": {
        "generic-lsp.path": {
          "type": "string",
          "default": "luajit",
          "description": "path to luajit"
        },
        "generic-lsp.arguments": {
          "type": "array",
          "default": [
            "./vscode/server/server.lua"
          ],
          "items": {
            "type": "string"
          },
          "description": "Arguments for Lua server"
        },
        "generic-lsp.extensions": {
          "description": "extensions",
          "type": "array",
          "default": [
            "lua",
            "oh"
          ],
          "items": {
            "type": "string"
          }
        },
        "generic-lsp.ip": {
          "type": "string",
          "default": "127.0.0.1",
          "description": "ip"
        },
        "generic-lsp.port": {
          "type": "string",
          "default": 1337,
          "description": "port"
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-extension-samples"
  },
  "engines": {
    "vscode": "^1.43.0"
  },
  "dependencies": {
    "vscode-languageclient": "^6.1.3"
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "update-vscode": "node ./node_modules/vscode/bin/install",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "main": "./out/extension",
  "devDependencies": {
    "@types/vscode": "1.43.0",
    "vscode-test": "^1.3.0",
    "@types/node": "^12.11.7",
    "eslint": "^6.4.0",
    "@typescript-eslint/parser": "^2.3.0",
    "typescript": "^3.8.3"
  }
}