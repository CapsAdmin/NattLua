{
    "name": "nattlua",
    "description": "Nattlua Language Client",
    "icon": "icon.png",
    "author": "CapsAdmin",
    "license": "MIT",
    "version": "0.0.1",
    "publisher": "unpublished",
    "keywords": [
        "LSP",
        "Language Server Client"
    ],
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:lua",
        "workspaceContains:**/*.lua",
        "onLanguage:nlua",
        "workspaceContains:**/*.nlua"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Microsoft/vscode-extension-samples"
    },
    "engines": {
        "vscode": "^1.64.0"
    },
    "main": "./out/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Nattlua configuration",
            "properties": {
                "nattlua.path": {
                    "type": "string",
                    "default": "/usr/local/bin/nattlua",
                    "description": "path to executable"
                },
                "nattlua.arguments": {
                    "type": "array",
                    "default": [
                        "language-server",
                        "1337"
                    ],
                    "items": {
                        "type": "string"
                    },
                    "description": "arguments for executable"
                },
                "nattlua.extensions": {
                    "description": "extensions",
                    "type": "array",
                    "default": [
                        "lua",
                        "nlua"
                    ],
                    "items": {
                        "type": "string"
                    }
                },
                "nattlua.ip": {
                    "type": "string",
                    "default": "127.0.0.1",
                    "description": "ip"
                },
                "nattlua.port": {
                    "type": "string",
                    "default": 1337,
                    "description": "port"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "yarn compile",
        "compile": "tsc -b",
        "watch": "tsc -b -w",
        "build": "npx vsce package",
        "test": "sh ./scripts/e2e.sh"
    },
    "dependencies": {
        "vscode-languageclient": "^7.0.0"
    },
    "devDependencies": {
        "@types/node": "^17.0.18",
        "@types/vscode": "^1.64.0",
        "typescript": "^4.5.5",
        "vsce": "^2.8.0"
    }
}
